            <class name="com.DC.uitests.adc.execute.mediaManagement.budgetManager.AmazonBudgetPacingTest"/>
            <class name="com.DC.uitests.adc.execute.mediaManagement.budgetManager.WalmartBudgetPacingTest"/>
            <class name="com.DC.uitests.adc.execute.mediaManagement.budgetManager.InstacartBudgetPacingTest"/>
            <class name="com.DC.uitests.adc.execute.mediaManagement.budgetManager.CriteoBudgetPacingTest"/>
            <class name="com.DC.uitests.adc.execute.mediaManagement.budgetManager.AmazonDSPBudgetPacingTest"/>
