<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Execute section UI tests" thread-count="3">
    <listeners>
        <listener class-name="com.DC.utilities.PriorityInterceptorListener"/>
        <listener class-name="com.DC.utilities.Reporting"/>
    </listeners>

    <test name="Tests for Execute section">
        <classes>
            <class name="com.DC.uitests.adc.execute.mediaManagement.EnsembleTest"/>
